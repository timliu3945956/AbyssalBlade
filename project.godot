; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="ABYSSAL BLADE"
run/main_scene="res://Menu/LoadScreen/LoadGameScreen.tscn"
config/use_custom_user_dir=true
config/features=PackedStringArray("4.3", "Forward Plus")
run/max_fps=60
boot_splash/show_image=false
config/icon="res://Utilities/icon/game_icon_v2.png"

[autoload]

TransitionScreen="*res://Menu/transition_screen.tscn"
AudioPlayer="*res://Menu/audio_player.tscn"
GlobalCount="*res://Singleton/global_count.gd"
ConfigFileHandler="*res://Characters/ConfigFileHandler.gd"
Global="*res://Singleton/global.gd"
HitStopManager="*res://Singleton/hit_stop_manager.gd"
LoadManager="*res://Singleton/load_manager.gd"
InteractionManager="*res://Singleton/InteractionManager.tscn"
GlobalEvents="*res://Singleton/GlobalEvents.gd"
SteamGlobal="*res://Singleton/SteamGlobal.gd"
SpeedController="*res://Singleton/SpeedController.gd"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"
InputManager="*res://Singleton/InputManager.tscn"
TimeWait="*res://Singleton/time_wait.gd"
SteamControllerInput="res://Singleton/SteamControllerInput.gd"
ButtonCapture="*res://Singleton/ButtonCapture.gd"
EffectManager="*res://Singleton/GlobalEffectManager.gd"

[debug]

file_logging/enable_file_logging=true

[dialogue_manager]

general/balloon_path="res://dialogue/balloon.tscn"

[display]

window/size/viewport_width=480
window/size/viewport_height=270
window/size/mode=4
window/size/window_width_override=1440
window/size/window_height_override=810
window/stretch/mode="canvas_items"
mouse_cursor/custom_image="res://sprites/mouse cursor/Mouse_Cursor_white.png"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_manager/plugin.cfg", "res://addons/smoothing/plugin.cfg")

[file_customization]

folder_colors={
"res://Boss0States/": "red",
"res://Boss2States/": "red",
"res://Boss3States/": "red",
"res://Boss4FinalStates/": "red",
"res://Boss4States/": "red",
"res://BossStates/": "red",
"res://Characters/": "blue",
"res://Singleton/": "gray",
"res://Utilities/": "yellow",
"res://sprites/": "green"
}

